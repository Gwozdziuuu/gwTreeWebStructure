<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>	
	<class name="com.gwozdz.hibernate.data.Tree" table="tree">
		<id name="id" column="id" type="long">
			<generator class="native" />
		</id>
		<property name="nodeData" column="nodeData" type="string"/>
		<property name="parentId" column="parentId" type="long"/>
		<property name="nodeValue" column="nodeValue" type="long"/>
	</class>
</hibernate-mapping>